@model Trabalho_Laboratorio.Models.Administrador

@{
	ViewData["Title"] = "Details";
}

<h1>Detalhes do Administrador</h1>

<div class="container">
	<div class="row">
		<div>
			<h4>Administrador</h4>
			<hr />
			<dl class="row">
				<dt class="col-sm-2">
					@Html.DisplayNameFor(model => model.Nome)
				</dt>
				<dd class="col-sm-10">
					@Html.DisplayFor(model => model.Nome)
				</dd>
				<dt class="col-sm-2">
					@Html.DisplayNameFor(model => model.Apelido)
				</dt>
				<dd class="col-sm-10">
					@Html.DisplayFor(model => model.Apelido)
				</dd>
				<dt class="col-sm-2">
					@Html.DisplayNameFor(model => model.Foto)
				</dt>
				<dd class="col-sm-10">
					<img class="img-thumbnail" src="@Html.DisplayFor(model => model.Foto)" />
				</dd>
				<dt class="col-sm-2">
					@Html.DisplayNameFor(model => model.Telefone)
				</dt>
				<dd class="col-sm-10">
					@Html.DisplayFor(model => model.Telefone)
				</dd>
				<dt class="col-sm-2">
					@Html.DisplayNameFor(model => model.IdAdminNavigation)
				</dt>
				<dd class="col-sm-10">
					@Html.DisplayFor(model => model.IdAdminNavigation.Email)
				</dd>
			</dl>
		</div>
	</div>
</div>